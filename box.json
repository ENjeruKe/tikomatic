{
    "chmod": "0755",
    "directories": [
        "src",
        "locale"
    ],
    "files": [
        "LICENSE",
        "vendor/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [
        {
            "name": "*.*",
            "exclude": [
            	"Tests", 
            	"tests", 
            	"build",
            	"phpunit", 
            	"phpspec",
            	"phpdocumentor",
            	"sebastian",
            	"doctrine",
            	"mockery"
            ],
            "in": ["vendor"]
        }
    ],
    "git-version": "package_version",
    "main": "src/index.php",
    "output": "build/tikomatic.phar",
    "stub": true
}